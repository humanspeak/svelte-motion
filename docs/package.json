{
    "name": "docs",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "cf-typegen": "wrangler types src/worker-configuration.d.ts",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "deploy": "pnpm run build && wrangler pages deploy",
        "dev": "vite dev",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "prepare": "svelte-kit sync || echo ''",
        "preview": "pnpm run build && wrangler pages dev",
        "sitemap:manifest": "node ./scripts/generate-sitemap-manifest.mjs",
        "sitemap:watch": "chokidar 'src/routes/**/*.svelte' 'src/routes/**/*.svx' 'src/routes/**/*.md' --ignore 'src/routes/examples/+page.ts' -c 'node ./scripts/generate-sitemap-manifest.mjs' --initial --silent",
        "test": "npm run test:unit -- --run",
        "test:unit": "vitest"
    },
    "dependencies": {
        "@humanspeak/svelte-motion": "workspace:*",
        "@inlang/paraglide-js": "^2.4.0",
        "@sentry/sveltekit": "^10.17.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1.4.0",
        "@eslint/js": "^9.37.0",
        "@sveltejs/adapter-auto": "^6.1.1",
        "@sveltejs/adapter-cloudflare": "^7.2.4",
        "@sveltejs/kit": "^2.43.8",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.14",
        "@vitest/browser": "^3.2.4",
        "chokidar-cli": "^3.0.0",
        "clsx": "^2.1.1",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-svelte": "3.12.4",
        "eslint-plugin-unused-imports": "4.2.0",
        "globals": "^16.4.0",
        "mdsvex": "^0.12.6",
        "mode-watcher": "^1.1.0",
        "playwright": "^1.55.1",
        "prettier": "^3.6.2",
        "prettier-plugin-sort-json": "^4.1.1",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "shiki": "^3.13.0",
        "svelte": "^5.39.8",
        "svelte-check": "^4.3.2",
        "tailwindcss": "^4.1.14",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.9",
        "vite-plugin-devtools-json": "^1.0.0",
        "vitest": "^3.2.4",
        "vitest-browser-svelte": "^1.1.0",
        "wrangler": "^4.42.0"
    },
    "pnpm": {}
}
