{
    "name": "docs",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "node ./scripts/generate-registry.mjs && vite build",
        "cf-typegen": "wrangler types src/worker-configuration.d.ts",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "deploy": "pnpm run build && wrangler pages deploy",
        "dev": "vite dev",
        "format": "prettier --write .",
        "lint": "prettier --check . && eslint .",
        "prepare": "svelte-kit sync || echo ''",
        "preview": "pnpm run build && wrangler pages dev",
        "registry:generate": "node ./scripts/generate-registry.mjs",
        "registry:watch": "chokidar 'src/lib/shadcn/ui/**/*.svelte' 'src/lib/shadcn/ui/**/*.ts' -c 'node ./scripts/generate-registry.mjs' --initial --silent",
        "sitemap:manifest": "node ./scripts/generate-sitemap-manifest.mjs",
        "sitemap:watch": "chokidar 'src/routes/**/*.svelte' 'src/routes/**/*.svx' 'src/routes/**/*.md' --ignore 'src/routes/examples/+page.ts' -c 'node ./scripts/generate-sitemap-manifest.mjs' --initial --silent",
        "test": "npm run test:unit -- --run",
        "test:unit": "vitest"
    },
    "dependencies": {
        "@humanspeak/svelte-motion": "workspace:*",
        "@inlang/paraglide-js": "^2.12.0",
        "@sentry/sveltekit": "^10.39.0",
        "github-slugger": "^2.0.0"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.2",
        "@eslint/js": "^9.39.2",
        "@sveltejs/adapter-auto": "^7.0.1",
        "@sveltejs/adapter-cloudflare": "^7.2.8",
        "@sveltejs/kit": "^2.52.2",
        "@sveltejs/vite-plugin-svelte": "^6.2.4",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.2.0",
        "@vitest/browser": "^4.0.18",
        "chokidar-cli": "^3.0.0",
        "clsx": "^2.1.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-svelte": "3.15.0",
        "eslint-plugin-unused-imports": "4.4.1",
        "globals": "^17.3.0",
        "mdsvex": "^0.12.6",
        "mode-watcher": "^1.1.0",
        "playwright": "^1.58.2",
        "prettier": "^3.8.1",
        "prettier-plugin-sort-json": "^4.2.0",
        "prettier-plugin-svelte": "^3.5.0",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "shiki": "^3.22.0",
        "svelte": "^5.53.0",
        "svelte-check": "^4.4.1",
        "tailwind-merge": "^3.5.0",
        "tailwind-variants": "^3.2.2",
        "tailwindcss": "^4.2.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0",
        "vite": "^7.3.1",
        "vite-plugin-devtools-json": "^1.0.0",
        "vitest": "^4.0.18",
        "vitest-browser-svelte": "^2.0.2",
        "wrangler": "^4.66.0"
    },
    "volta": {
        "node": "24.12.0"
    },
    "pnpm": {}
}
